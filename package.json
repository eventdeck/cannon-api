{
  "name": "cannon-api",
  "version": "0.0.0",
  "description": "The ultimate webapp for the ultimate event!",
  "main": "server/index.js",
  "scripts": {
    "run": "NODE_PATH=. nodemon server/index.js | bunyan",
    "start": "NODE_PATH=. node server/index.js | bunyan",
    "mon": "NODE_PATH=. nodemon server/index.js | bunyan",
    "test": "NODE_PATH=. node_modules/lab/bin/lab server/test -l  -m 2000 --verbose",
    "test-cov": "NODE_PATH=. node_modules/lab/bin/lab server/test -c",
    "lint": "node_modules/.bin/jshint -c .jshintrc ."
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/sinfo/cannon-api.git"
  },
  "author": "SINFO Dev Team <devteam@sinfo.org> (https://github.com/sinfo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sinfo/cannon-api/issues"
  },
  "homepage": "https://github.com/sinfo/cannon-api",
  "dependencies": {
    "async": "^0.9.0",
    "boom": "~2.5.1",
    "bunyan": "~1.2.1",
    "fenixedu": "https://github.com/jgantunes/fenixedu/tarball/master",
    "good": "^2.3.0",
    "handlebars": "^2.0.0",
    "hapi": "~7.5.0",
    "hapi-auth-basic": "^1.1.1",
    "hapi-auth-bearer-token": "^2.2.2",
    "hapi-swagger": "~0.4.2",
    "hoek": "^2.11.0",
    "joi": "^6.0.8",
    "jsonwebtoken": "^3.2.2",
    "mime": "^1.2.11",
    "moment": "^2.9.0",
    "mongoose": "^3.8.16",
    "nodemailer": "^1.3.0",
    "nodemailer-sendmail-transport": "^1.0.0",
    "nodepdf": "^1.1.3",
    "parse-http-header": "^1.0.0",
    "qr-image": "~2.0.0",
    "qs": "^2.3.3",
    "request": "^2.44.0",
    "slug": "~0.7.1",
    "underscore": "^1.8.1",
    "urlencode": "^0.2.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "nodemon": "~1.2.1",
    "code": "~1.2.0",
    "lab": "~5.0.1",
    "jshint": "~2.5.10",
    "pre-commit": "^0.0.11"
  }
